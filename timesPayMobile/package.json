    "run-android:prd": "cross-env NODE_ENV=production npx react-native run-android",
    "run-android": "cross-env NODE_ENV=development npx react-native run-android",
    "run-ios:prd": "cross-env NODE_ENV=production npx react-native run-ios",
    "run-ios": "cross-env NODE_ENV=development npx react-native run-ios",
    "start:prd": "cross-env NODE_ENV=production npx react-native start",
